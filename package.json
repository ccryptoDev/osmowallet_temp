    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/multer": "^1.4.7",
